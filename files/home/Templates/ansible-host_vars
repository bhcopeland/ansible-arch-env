sudoers_root_acl:
 - user.name

sudoers_access_acl:
 - user.name

sudoers_logaccess:
 - logaccess

sudoers_access:
     - /foo/bar/*
     - /foo/bin

sudoers_root:
 - (ALL) ALL

network_ether_interfaces:
       - device: em1
         bootproto: static
         address: 172.16.1.4
         netmask: 255.255.0.0
         gateway: 172.16.0.1
         nameservers: 172.16.0.1 8.8.8.8 8.8.4.4